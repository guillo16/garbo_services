<h2 class="py-5 py-md-6 text-center" id="page-title">Noticias</h2>
<div class="container">
  <div class="container post-container">
    <% @posts.each do |post| %>
      <%= render 'shared/cards', post: post %>
    <% end %>
  </div>
  <div class="text-center py-5">
    <%= link_to "Mas noticias", posts_path, class: "btn btn-success pt-2 px-4  mb-md-5 text-white"  %>
  </div>
</div>
